@model IdentityRole

@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<form asp-controller="Role" asp-action="Create" class="form-group">
    <label asp-for="Name">Role Name</label>
    <input asp-for="Name" type="text" placeholder="Role Name" class="form-control" />
    <br />
    <input type="submit" value="Save" class="btn btn-danger" />
</form>

