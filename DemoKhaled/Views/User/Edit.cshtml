@model IdentityUser
@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<form class="form-group" asp-controller="User" asp-action="Edit" method="post">
    <label asp-for="Id">ID</label>
    <input asp-for="Id" type="text" readonly class="form-control" />
    <br />
    <label>Email</label>
    <input asp-for="Email" type="text" class="form-control" />
    <br />
    <label asp-for="Id">User Name</label>
    <input asp-for="UserName" type="text" class="form-control" />
    <br />
    <input type="submit" class="btn btn-danger" />
</form>

